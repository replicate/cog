# Test that cog exec /bin/bash works with interactive PTY
# This verifies bidirectional PTY interaction (send input, receive output)

# Run bash with PTY input file - send commands and verify output
# cog exec builds from current directory and runs the command
pty-run pty_input.txt cog exec /bin/bash
stdout 'SENTINEL_12345'
stdout 'HELLO_WORLD'

-- cog.yaml --
build:
  python_version: "3.12"
run: "run.py:Runner"

-- run.py --
from cog import BaseRunner

class Runner(BaseRunner):
    def run(self, s: str = "world") -> str:
        return "hello " + s

-- pty_input.txt --
echo SENTINEL_12345
echo HELLO_WORLD
exit
