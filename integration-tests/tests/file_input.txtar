# Build the image
cog build -t $TEST_IMAGE

# File input works
cog run $TEST_IMAGE -i file=@input.txt
stdout 'hello from file'

-- cog.yaml --
build:
  python_version: "3.12"
run: "run.py:Runner"

-- run.py --
from cog import BaseRunner, Path

class Runner(BaseRunner):
    def run(self, file: Path) -> str:
        return file.read_text()

-- input.txt --
hello from file
