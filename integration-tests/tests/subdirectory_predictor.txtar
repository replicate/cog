
# Test runner in subdirectory with imports from parent

# Build the image
cog build -t $TEST_IMAGE

# Run using runner in subdirectory
cog run $TEST_IMAGE -i s=world
stdout 'hello world'

-- cog.yaml --
build:
  python_version: "3.12"
run: "my-subdir/run.py:Runner"

-- mylib.py --
def concat(a, b):
    return a + " " + b

-- my-subdir/run.py --
from cog import BaseRunner
from mylib import concat


class Runner(BaseRunner):
    def run(self, s: str) -> str:
        return concat("hello", s)
