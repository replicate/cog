
# Build image
cog build -t $TEST_IMAGE

# Run using class without BaseRunner
cog run $TEST_IMAGE -i text=world
stdout 'hello world'

-- cog.yaml --
build:
  python_version: "3.12"
run: run.py:Predictor

-- run.py --
from cog import Input


class Predictor:
    def run(self, text: str = Input(default="world")) -> str:
        return f"hello {text}"
