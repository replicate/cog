
# Build image
cog build -t $TEST_IMAGE

# Predict using standalone function
cog run $TEST_IMAGE -i text=world
stdout 'hello world'

-- cog.yaml --
build:
  python_version: "3.12"
run: run.py:run

-- run.py --
from cog import Input


def run(text: str = Input(default="world")) -> str:
    return f"hello {text}"
