# Test Python 3.13 support

# Build the image
cog build -t $TEST_IMAGE

# Predict with Python 3.13
cog run $TEST_IMAGE -i num=5
stdout '10'

-- cog.yaml --
build:
  gpu: false
  python_version: "3.13"
run: "run.py:Runner"

-- run.py --
from cog import BaseRunner


class Runner(BaseRunner):
    def run(self, num: int) -> int:
        return num * 2
