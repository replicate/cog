# Test list[str] input type

# Build the image
cog build -t $TEST_IMAGE

# Predict with list input
cog run $TEST_IMAGE -i s=world
stdout 'hello world'

-- cog.yaml --
build:
  python_version: "3.12"
run: "run.py:Runner"

-- run.py --
from cog import BaseRunner, Input


class Runner(BaseRunner):
    def run(self, s: list[str] = Input(description="A list of strings to print.")) -> str:
        return "hello " + "|".join(s)
