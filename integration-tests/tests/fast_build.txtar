# Test that fast build mode works (with fast: true in cog.yaml)

# Run prediction with fast build mode
cog predict -i s=world
stdout 'hello world'

-- cog.yaml --
build:
  python_version: "3.12"
  python_requirements: requirements.txt
  system_packages:
    - "git"
  fast: true
predict: "predict.py:Predictor"

-- requirements.txt --
torch==2.5.0
beautifulsoup4==4.12.3

-- predict.py --
from cog import BasePredictor

class Predictor(BasePredictor):
    test_inputs = {"s": "world"}

    def predict(self, s: str) -> str:
        return "hello " + s
