# Build the image
cog build -t $TEST_IMAGE

# Async prediction works
cog run $TEST_IMAGE -i s=world
stdout 'hello world'

-- cog.yaml --
build:
  python_version: "3.12"
run: "run.py:Runner"

-- run.py --
from cog import BaseRunner

class Runner(BaseRunner):
    async def run(self, s: str) -> str:
        return "hello " + s
