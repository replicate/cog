# Build the image
cog build -t $TEST_IMAGE

# Async prediction works
cog predict $TEST_IMAGE -i s=world
stdout 'hello world'

-- cog.yaml --
build:
  python_version: "3.12"
predict: "predict.py:Predictor"

-- predict.py --
from cog import BasePredictor

class Predictor(BasePredictor):
    async def predict(self, s: str) -> str:
        return "hello " + s
