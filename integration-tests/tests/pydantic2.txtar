# Test explicit Pydantic 2 dependency

# Build the image
cog build -t $TEST_IMAGE

# Predict with Pydantic 2
cog run $TEST_IMAGE -i s=world
stdout 'hello world'

-- cog.yaml --
build:
  gpu: false
  python_version: "3.12"
  python_packages:
    - "pydantic>2"
run: "run.py:Runner"

-- run.py --
from cog import BaseRunner


class Runner(BaseRunner):
    def run(self, s: str) -> str:
        return "hello " + s
