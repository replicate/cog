# Build the image
cog build -t $TEST_IMAGE

# Environment variables are set
cog run $TEST_IMAGE -i name=TEST_VAR
stdout 'test_value'

cog run $TEST_IMAGE -i name=NAME
stdout 'michael'

-- cog.yaml --
run: "run.py:Runner"
build:
  python_version: "3.12"
environment:
  - NAME=michael
  - TEST_VAR=test_value

-- run.py --
from cog import BaseRunner
import os

class Runner(BaseRunner):
    def run(self, name: str) -> str:
        return f"ENV[{name}]={os.getenv(name)}"
