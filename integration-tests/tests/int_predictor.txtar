# Build the image
cog build -t $TEST_IMAGE

# Integer input and output works
cog run $TEST_IMAGE -i num=5
stdout '10'

# Negative numbers work
cog run $TEST_IMAGE -i num=-3
stdout '-6'

-- cog.yaml --
build:
  python_version: "3.12"
run: "run.py:Runner"

-- run.py --
from cog import BaseRunner

class Runner(BaseRunner):
    def run(self, num: int) -> int:
        return num * 2
